{
  "name": "webapp-starter",
  "version": "1.0.0",
  "description": "webapp starter kit ",
  "main": "index.js",
  "author": "Brad Stapleton",
  "license": "MIT",
  "scripts": {
    "dev": "NODE_ENV=dev ./node_modules/.bin/webpack-dev-server",
    "start:dev": "npm run dev -- --watch ",
    "build": "NODE_ENV=production ./node_modules/.bin/webpack",
    "testdev": "NODE_ENV=development ./node_modules/.bin/webpack --watch",
    "build:watch": "NODE_ENV=production ./node_modules/.bin/webpack  && npm run postbuild",
    "postbuild": "NODE_ENV=dev ./node_modules/.bin/webpack-dev-server --watch"
  },
  "devDependencies": {
    "autoprefixer": "^8.3.0",
    "babel-core": "^6.26.2",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.5.1",
    "css-loader": "^0.28.11",
    "file-loader": "^1.1.11",
    "image-webpack-loader": "^4.2.0",
    "imagemin-webpack-plugin": "^2.1.1",
    "img-loader": "^2.0.1",
    "mini-css-extract-plugin": "^0.4.0",
    "node-sass": "^4.9.0",
    "popper.js": "^1.14.3",
    "postcss-loader": "^2.1.4",
    "purify-css": "^1.2.5",
    "purifycss-webpack": "^0.7.0",
    "raw-loader": "^0.5.1",
    "sass-loader": "^7.0.1",
    "style-loader": "^0.21.0",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "url-loader": "^1.0.1",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.0.15",
    "webpack-dev-server": "^3.1.3"
  },
  "dependencies": {
    "bootstrap": "^4.1.0",
    "jquery": "^3.3.1"
  }
